{
  "presets": [
    "stage-1",
    "react",
    "es2016"
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    "add-module-exports",
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": false,
      "preprocess": true,
      "minify": true,
      "transpileTemplateLiterals": false
    }]
  ]
}